<div class = 'page-main'>
    <h1>Create New Course:</h1>


    <%= form_for @course do |f| %>
        <%= f.label :name %><br>
        <%= f.text_field :name %><br>

        <%= f.collection_select :course_type, @course_types, :itself, :itself %><br>


        <%= f.label :description %><br>
        <%= f.text_area :description %><br>
        
        <%= f.submit %>
    <% end %>
</div>